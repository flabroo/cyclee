<div class="cyclee-card">
  <div class="cyclee-card-image">
    <%= report.photo.present? ? (cl_image_tag report.photo.key, crop: :fill) : (image_tag "greenlane.png", crop: :fill) %>
  </div>
  <div class="cyclee-card-contents">
    <div class="cyclee-card-user">
      <%= image_tag "logo.png" %>
    </div>
    <div class="text-content">
      <p><strong><%= report.user.first_name %> <%= report.user.last_name %></strong></p>
      <p class="incident"><%= report.date %></p>
      <p class="text-content"><%= report.comment %></p>
    </div>
  </div>
</div>
